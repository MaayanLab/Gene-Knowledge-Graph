    "header": {
        "header": "Biomarker-KG",
        "title": "Biomarker-KG",
        "background": {
            "backgroundColor": "#000",
            "contrastText": "#FFF"
        },
        "icon": {
            "src": "/icon/bkg.png",
            "favicon": "/icon/bkg.png",
            "faviconTitle": "Biomarker-KG",
            "alt": "Biomarker-KG",
            "key": "Biomarker-KG_logo",
            "width": 100,
            "height": 100
        },
        "tabs": [
            {
                "endpoint": "/",
                "label": "BKG Explorer",
                "type": "page",
                "component": "KnowledgeGraph",
                "props": {
                    "init_function": "initialize_kg",
                    "title": "BKG Explorer: Find connections between different BKG entities.",
                    "description": "Search two BKG entities or one entity to see related BKG entities.",
                    "initial_query": {
                        "start": "Condition",
                        "start_term": "lung carcinoma",
                        "start_field": "label",
                        "end": "Anatomy",
                        "end_term": "blood",
                        "end_field": "label"
                    },
                    "subheader": {
                        "url_field": "filter",
                        "query_field": "relation"
                    }
                }
            },
            {
                "endpoint": "/downloads",
                "label": "Download",
                "type": "page",
                "component": "Download",
                "props": {
                  "src": "https://raw.githubusercontent.com/nialingam/BiomarkerKGDemo/main/biomarker_downloads.json"
                }
            },
            {
                "endpoint": "/tutorials",
                "label": "Tutorials",
                "type": "markdown",
                "component": "Markdown",
                "props": {
                  "src": "https://raw.githubusercontent.com/nialingam/BiomarkerKGDemo/main/About3.md"
                }
            },
            {
                "endpoint": "/use_cases",
                "label": "Use Cases",
                "type": "group",
                "component": "DistilleryLanding",
                "props": {
                    "title": "BKG Use Cases",
                    "description": "Explore use cases that use data from the Biomarker Partnership",
                    "pages": [
                        {
                            "endpoint": "/use_cases/lungcarcinomarisk",
                            "label": "Lung Carcinoma Risk",
                            "type": "page",
                            "component": "DistilleryUseCase",
                            "props": {
                                "init_function": "initialize_kg",
                                "title": "Risks for Lung Carcinoma",
                                "icon": "/icon/lungcarc.png",
                                "description": "Explore the SNPs that indicate risk for lung carcinoma and the tissues they were collected from.",
                                "endpoint": "/api/biomarker/lungcarcinomarisk",
                                "type": "Condition",
                                "fields": [
                                    "Condition",
                                    "id",
                                    "label",
                                    "Anatomy",
                                    "Biomarker"
                                ],
                                "default_term": "lung carcinoma"
                            }
                        },
                        {
                            "endpoint": "/use_cases/brainandthyroid",
                            "label": "Common SNPs between Brain and Thyroid Cancer",
                            "type": "page",
                            "component": "DistilleryUseCase",
                            "props": {
                                "init_function": "initialize_kg",
                                "title": "Common SNPs between Brain and Thyroid Cancer",
                                "icon": "/icon/brainandthyroid.png",
                                "description": "See the SNPs that indicate risks for both thyroid and brain cancer.",
                                "endpoint": "/api/biomarker/brainandthyroid",
                                "type": "Condition",
                                "fields": [
                                    "Condition",
                                    "id",
                                    "label",
                                    "Biomarker"
                                ],
                                "default_term": "brain cancer"
                            }
                        },
                        {
                            "endpoint": "/use_cases/blooddiagnostic",
                            "label": "SNPs Collected from Blood and Conditions They Can Diagnose",
                            "type": "page",
                            "component": "DistilleryUseCase",
                            "props": {
                                "init_function": "initialize_kg",
                                "title": "SNPs Collected from Blood and Conditions They Can Diagnose",
                                "icon": "/icon/blood.png",
                                "description": "See the SNPs collected from blood and the conditions they are diagnostic for.",
                                "endpoint": "/api/biomarker/blooddiagnostic",
                                "type": "Condition",
                                "fields": [
                                    "Anatomy",
                                    "id",
                                    "label",
                                    "Biomarker",
                                    "Condition"
                                ],
                                "default_term": "blood"
                            }
                        },
                        {
                            "endpoint": "/use_cases/basalcellcarcinomarisk",
                            "label": "Mutations that Suggest Risk for Basal Cell Carcinoma",
                            "type": "page",
                            "component": "DistilleryUseCase",
                            "props": {
                                "init_function": "initialize_kg",
                                "title": "Mutations that Suggest Risk for Basal Cell Carcinoma",
                                "icon": "/icon/basalcell.png",
                                "description": "Explore the mutations that indicate risk for basal cell carcinoma.",
                                "endpoint": "/api/biomarker/basalcellcarcinomarisk",
                                "type": "Condition",
                                "fields": [
                                    "Anatomy",
                                    "id",
                                    "label",
                                    "Biomarker",
                                    "Variant"
                                ],
                                "default_term": "basal cell carcinoma"
                            }
                        }
                    ]
                }
            }
        ],
